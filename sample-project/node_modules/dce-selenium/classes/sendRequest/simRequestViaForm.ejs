<head>
  <title>Prepping POST request...</title>
</head>

<body>
  <form action="<%= url %>" method="<%= method %>">
    <% for (let prop in body) { %>
      <input type="hidden" name="<%= prop %>" value="<%= body[prop] %>">
    <% } %>
    <input type="submit" id="submit" value="Continue">
  </form>
</body>
